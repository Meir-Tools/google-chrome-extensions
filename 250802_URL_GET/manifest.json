{
  "manifest_version": 3,
  "name": "GET URL Params Reader",
  "version": "1.0",
  "description": "קורא פרמטרים מסוג GET מה-URL",
  "permissions": ["tabs", "scripting"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {

    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
